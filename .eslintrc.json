{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "linebreak-style": "off",
    "indent": "off",
    "eol-last":"off", 
    "no-undef":"warn", 
    "no-unused-expressions": "warn",
    "no-underscore-dangle": "off", 
    "no-unused-vars": "warn",
    "no-cycle": "off"
  }
}
