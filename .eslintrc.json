{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": { 
      "linebreak-style": "off",
      "indent": "off",
      "eol-last": "off",
      "no-undef": "off",
      "no-unused-expressions": "warn",
      "no-underscore-dangle": "off",
      "no-unused-vars": "warn",
      "no-cycle": "off",
      "comma-dangle": "off",
      "operator-linebreak": "off"
    }
}
